<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListadoClientes</servlet-name>
  	<display-name>ListadoClientes</display-name>
  	<description></description>
  	<servlet-class>com.curso.banco.web.ListadoClientes</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CuentaCliente</servlet-name>
  	<display-name>CuentaCliente</display-name>
  	<description></description>
  	<servlet-class>com.curso.banco.web.CuentaCliente</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListadoClientes</servlet-name>
  	<url-pattern>/ListadoClientes</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CuentaCliente</servlet-name>
  	<url-pattern>/CuentaCliente</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
